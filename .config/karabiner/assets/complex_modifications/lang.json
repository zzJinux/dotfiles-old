{
    "title": "Languages",
    "rules": [
        {
            "description": "F16 to option + space",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f16",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "spacebar",
                            "modifiers": [
                                "left_alt"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "(VIM) ESC to Apple_EN",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "escape",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "select_input_source": {
                                "input_source_id": "^com\\.apple\\.keylayout\\.ABC$"
                            }
                        }
                    ],
                    "conditions": [
                        {
                            "type": "input_source_unless",
                            "input_sources": [
                                {
                                    "input_source_id": "^com\\.apple\\.keylayout\\.ABC$"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "(VIM) Ctrl+[ to Apple_EN",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "open_bracket",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "select_input_source": {
                                "input_source_id": "^com\\.apple\\.keylayout\\.ABC$"
